{
  "manifest_version": 3,
  "name": "youtube-better-ambient",
  "version": "1.0.10",
  "description": "Enhances YouTubeâ€™s ambient mode by placing a blurred version of the video in the background for a more immersive experience",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "js": [
        "dist/static/js/script.js"
      ]
    }
  ],
  "icons": {
    "64": "icons/icon-64.png",
    "128": "icons/icon-128.png",
    "192": "icons/icon-192.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "pr0verter@gmail.com",
      "strict_min_version": "109.0"
    }
  },
  "action": {
    "default_popup": "dist/popup.html"
  },
  "permissions": [
    "storage"
  ] 
}